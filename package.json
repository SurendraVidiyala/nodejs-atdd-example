{
  "name": "nodejs-atdd-example",
  "version": "0.0.1",
  "description": "ATDD Example",
  "main": "bin/server.js",
  "scripts": {
    "start": "node ./src/start.js",
    "test-unit": "./node_modules/.bin/mocha --recursive test-unit",
    "test-integration": "./node_modules/.bin/mocha --recursive test-integration",
    "test-acceptance": "./node_modules/.bin/cucumber.js test-acceptance/features -r test-acceptance/step_definitions --tags ~@skip --format pretty --strict",
    "test-coverage": "node node_modules/istanbul/lib/cli.js cover ./node_modules/.bin/cucumber.js --lcovonly -- test-acceptance/features  -r test-acceptance/step_definitions --tags ~@skip && istanbul-coveralls"
  },
  "pre-commit": [
    "style",
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/IGZangelsanchez/nodejs-atdd-example.git"
  },
  "keywords": [
    "nodejs",
    "api",
    "template"
  ],
  "author": "Angel Sanchez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IGZangelsanchez/nodejs-atdd-example/issues"
  },
  "homepage": "https://github.com/IGZangelsanchez/nodejs-atdd-example",
  "dependencies": {
    "mongodb": "2.0.45",
    "restify": "4.0.3",
    "tv4": "^1.2.7",
    "winston": "1.1.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "cucumber": "0.6.0",
    "istanbul": "^0.4.2",
    "istanbul-coveralls": "^1.0.3",
    "mocha": "2.3.3",
    "mockery": "1.4.0",
    "request": "2.64.0",
    "sinon": "1.17.1"
  },
  "engines": {
    "node": ">= 4.2.2"
  }
}
